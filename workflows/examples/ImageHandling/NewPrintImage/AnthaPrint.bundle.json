{
  "Config": {
    "UseDriverTipTracking": true
  },
  "Parameters": {
    "Add_Solution_Multi": {
      "Names": [
        "blue",
        "yellow",
        "green",
        "red",
        "black",
        "transparent",
        "white"
      ],
      "UseLHPolicy": {
        "default": "water"
      }
    },
    "ChoosePalette": {
      "AvailableColors": [],
      "PaletteID": "Paints"
    },
    "CreateAnthaPalette": {
      "LowerThreshold": 50
    },
    "DownloadImage": {
      "URL": "https://www.petfinder.com/wp-content/uploads/2012/11/bird-average-bird-lifespans-thinkstock-155253666-632x457.jpg"
    },
    "ExportResizedImage": {
      "OutputImageFileName": "Resized"
    },
    "MakeAnthaImage": {
      "Plate": "greiner384"
    },
    "PrintAnthaImage": {
      "ColorToExclude" : "white"
    }
  },
  "Processes": {
    "Add_Solution_Multi": {
      "Metadata": {
        "skin": "light"
      },
      "component": "Add_Solution_Multi"
    },
    "ChoosePalette": {
      "Metadata": {
        "skin": "light"
      },
      "component": "ChoosePalette"
    },
    "CreateAnthaPalette": {
      "Metadata": {
        "skin": "light"
      },
      "component": "CreateAnthaPalette"
    },
    "DownloadImage": {
      "Metadata": {
        "skin": "light"
      },
      "component": "DownloadImage"
    },
    "ExportResizedImage": {
      "Metadata": {
        "skin": "light"
      },
      "component": "ExportImage"
    },
    "MakeAnthaImage": {
      "Metadata": {
        "skin": "light"
      },
      "component": "MakeAnthaImage"
    },
    "MakeAnthaPalette": {
      "Metadata": {
        "skin": "light"
      },
      "component": "MakeAnthaPalette"
    },
    "PrintAnthaImage": {
      "Metadata": {
        "skin": "light"
      },
      "component": "PrintAnthaImage"
    }
  },
  "Properties": {
    "description": "This is a workflow to print a digital image with paint",
    "id": "---",
    "name": "AnthaPrint"
  },
  "connections": [
    {
      "src": {
        "port": "Palette",
        "process": "ChoosePalette"
      },
      "tgt": {
        "port": "Palette",
        "process": "MakeAnthaPalette"
      }
    },
    {
      "src": {
        "port": "NewSolutions",
        "process": "Add_Solution_Multi"
      },
      "tgt": {
        "port": "LHComponents",
        "process": "MakeAnthaPalette"
      }
    },
    {
      "src": {
        "port": "AnthaPalette",
        "process": "MakeAnthaPalette"
      },
      "tgt": {
        "port": "AnthaPalette",
        "process": "CreateAnthaPalette"
      }
    },
    {
      "src": {
        "port": "Image",
        "process": "DownloadImage"
      },
      "tgt": {
        "port": "InputImage",
        "process": "MakeAnthaImage"
      }
    },
    {
      "src": {
        "port": "MixedAnthaPalette",
        "process": "CreateAnthaPalette"
      },
      "tgt": {
        "port": "AnthaPalette",
        "process": "MakeAnthaImage"
      }
    },
    {
      "src": {
        "port": "AnthaImage",
        "process": "MakeAnthaImage"
      },
      "tgt": {
        "port": "AnthaImage",
        "process": "PrintAnthaImage"
      }
    },
    {
      "src": {
        "port": "ResizedImage",
        "process": "MakeAnthaImage"
      },
      "tgt": {
        "port": "Image",
        "process": "ExportResizedImage"
      }
    }
  ]
}